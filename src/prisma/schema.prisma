// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model air_quality {
  id           Int      @id @default(autoincrement()) 
  datetime     DateTime 
  co_gt        Float    
  pt08_s1_co   Int      
  nmhc_gt      Int      
  c6h6_gt      Float    
  pt08_s2_nmhc Int      
  nox_gt       Int      
  pt08_s3_nox  Int      
  no2_gt       Int      
  pt08_s4_no2  Int      
  pt08_s5_o3   Int      
  t            Float    
  rh           Float    
  ah           Float    
}